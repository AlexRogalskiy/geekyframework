{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": ["**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": [
      false,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ]
  },
  "jsRules": {
    "object-literal-sort-keys": false, //Disable for javascript
    "no-console": [
      false,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ]
  }
}
